@model Telimena.WebApp.Models.Shared.ProgramHeaderMenuViewModel
<section class="content-header">
    <h1>
        @Model.ProgramName
        <small>
            <span >
                <a class="headerMenuLink" href="@Url.Action("Dashboard", "ProgramStatistics", new {telemetryKey = Model.TelemetryKey})" id="headerMenuDashboardLink">
                    @*<i class="fa fa-cog"></i>*@ 
                    <span>Dashboard</span>
                </a>
            </span>
            <span>&nbsp;|&nbsp;</span>
            <span >
                <a class="headerMenuLink" href="@Url.Action("Index", "ProgramManagement", new {telemetryKey = Model.TelemetryKey})" id="headerMenuManageLink">
                    @*<i class="fa fa-cog"></i>*@
                    <span>Manage</span>
